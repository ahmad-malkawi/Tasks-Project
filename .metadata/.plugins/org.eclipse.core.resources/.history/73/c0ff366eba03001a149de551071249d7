package jo.secondstepe.dataStructureTask;

public class Main {

	public static void main(String[] args) {
		SSSArayList list= new SSSArayList();
			list.add(5);
			list.add(6);
			list.print();
			list.add(1,7);
			list.print();
			list.remove(1);
			list.print();
			list.set(0, 4);
			list.print();
			list.clear();
			list.addFirst(11);
			list.print();
			list.add(3);
			list.add(5);
			list.add(6);
			list.print();
			System.out.println("the size l1: "+list.size());
			SSSArayList list2= new SSSArayList();
			list2.add(25);
			list2.add(26);
			list2.add(27);
			System.out.println("the size l2: "+list2.size());
			list.addAll(1,list2);
			list.print();

	}

}
