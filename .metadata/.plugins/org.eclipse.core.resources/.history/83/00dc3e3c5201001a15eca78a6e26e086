package jo.secondstep.session2;

import java.util.Arrays;
import java.util.List;
import java.util.Scanner;

public class Main {
	
	static Object[] rotLeft(int[] a, int d , int n) {
	       List aList = Arrays.asList(a);
	       for (int i=0; i< d ;i++){
	           aList.add(0,aList.get(n-1));
	           aList.remove(1);
	       }
	       return aList.toArray(new Integer[n]);
	    }

	public static void main(String[] args) {
		Student one = new Student();
		one.setStd_name();
		one.setStd_class();
		one.setSubjects_num();
		one.setSubjects_names();
		one.setSubjects_marks();
		one.printStuedentInfo();
		
	}
}