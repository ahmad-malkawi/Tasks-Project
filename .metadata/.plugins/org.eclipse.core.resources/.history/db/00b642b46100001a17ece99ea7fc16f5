package jo.secondstep.session3.second;

import java.util.ArrayList;
import java.util.Random;

public class SecondGame {
	public static void main(String[] args) {
		ArrayList<Boolean> colerList = new ArrayList<Boolean>(16); 
		ArrayList<Integer> randomList = new ArrayList<Integer>(4); 
		ArrayList<Integer> anserList = new ArrayList<Integer>(4);
		int temp;
		for (int i =0 ; i<16 ;i++) {
			colerList.set(i, true);
		}
		Random rand = new Random();
		for (int i = 0 ;i<4;i++) {
			randomList.add(rand.nextInt((16 - 1) + 1) + 1);
		}
		 
		
		
	}

}
