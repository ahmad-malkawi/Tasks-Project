package jo.secondstep.session2;

public class Subject {
	String name;
	double mark;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getMark() {
		return mark;
	}
	public void setMark(double subject_mark) {
		this.mark = subject_mark;
	}
}
