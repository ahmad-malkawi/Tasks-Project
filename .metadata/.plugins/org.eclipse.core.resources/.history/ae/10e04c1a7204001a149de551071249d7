package jo.secondstep.session6;

public class Employee extends Person {
	
	private int empID;
	private String role;
	
	public Employee(String Name,String dateOfPirth,int empID, String role) {
		super(Name,dateOfPirth);
		this.empID = empID;
		this.role = role;
	}
	public int getEmpID() {
		return empID;
	}
	public void setEmpID(int empID) {
		this.empID = empID;
	}
	public String getRole() {
		return role;
	}
	public void setRole(String role) {
		this.role = role;
	}
}
